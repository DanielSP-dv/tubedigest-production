{
  "_comment": "Force Vercel deployment - API rewrites configured for backend integration",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://tubedigest-backend-clean.onrender.com/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
